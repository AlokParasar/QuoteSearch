@model IEnumerable<QuoteSearch.Models.Quote>

<table class="table" width="100%">
    <tr>
        <th style="font-weight: bold; font-size:large; background-color:blueviolet">
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th style="font-weight: bold; font-size:large; background-color:blueviolet">
            @Html.DisplayNameFor(model => model.DoB)
        </th>
        <th style="font-weight: bold; font-size:large; background-color:blueviolet">Add to Quote</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoB)
            </td>
            <td>
                <a onclick="showDetails('@item.QuoteId');">Details</a>
            </td>
        </tr>
    }

</table>
